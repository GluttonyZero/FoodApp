{
    "version": 2,
    "builds": [
      {
        "src": "api/scrape.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/public/index.html"
      },
      {
        "src": "/(.*)",
        "dest": "/api/scrape.js"
      }
    ]
  }
  